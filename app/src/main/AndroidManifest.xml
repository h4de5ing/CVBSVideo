<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.twovideotest">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.RECORD_VIDEO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SettingStandardActivity"
            android:icon="@mipmap/ic_launcher_setting"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".VideoService" />

        <activity
            android:name=".PermissionActivity"
            android:exported="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name_setting"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.example.twovideotest.MainActivity"
            android:exported="true"
            android:label="@string/title_activity_zhi"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".TestTwoVideoActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".OneVideoActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".TwoVideoActivity"
            android:screenOrientation="landscape" />

    </application>

</manifest>